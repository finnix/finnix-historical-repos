#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@ 

override_dh_auto_install:
	install -m 0755 -d $(CURDIR)/debian/oui/usr/bin
	install -m 0755 -d $(CURDIR)/debian/oui/usr/share/man/man1
	install -m 0644 oui $(CURDIR)/debian/oui/usr/bin/oui
	install -m 0644 oui.1 $(CURDIR)/debian/oui/usr/share/man/man1/oui.1

override_dh_builddeb:
	dh_builddeb -- -Zxz
