#!/usr/bin/make -f
# -*- makefile -*-

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@

override_dh_auto_install:
	install -m 0755 -d $(CURDIR)/debian/neale-stuff
	cp -a $(CURDIR)/debian/stuff-files/common/* $(CURDIR)/debian/neale-stuff
	cp -a $(CURDIR)/debian/stuff-files/$(DEB_HOST_ARCH)/* $(CURDIR)/debian/neale-stuff
